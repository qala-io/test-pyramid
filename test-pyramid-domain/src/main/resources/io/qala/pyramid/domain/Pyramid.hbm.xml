<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="io.qala.pyramid.domain">
  <class name="Pyramid" table="PYRAMID">
    <id name="id">
      <generator class="uuid"/>
    </id>
    <property name="name"/>
    <property name="nOfUnitTests"/>
    <property name="nOfComponentTests"/>
    <property name="nOfSystemTests"/>
  </class>
</hibernate-mapping>